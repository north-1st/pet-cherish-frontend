    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "stylelint": "^16.4.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
